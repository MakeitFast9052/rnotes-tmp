{
  "$schema": "https://schema.tauri.app/config/2",
  "build": {
    "frontendDist": "../src"
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
        "./icons/128x128.png",
        "./icons/32x32.png",
        "./icons/icon.icns",
        "./icons/icon.ico",
        "./icons/icon.png"
    ]
  },
  "productName": "rnotes",
  "mainBinaryName": "rnotes",
  "version": "0.1.0",
  "identifier": "com.makeitfast9052.rnotes",
  "plugins": {},
  "app": {
    "withGlobalTauri": true,
    "windows": [
      {
        "title": "Rote Notes",
        "width": 540,
        "height": 700,
        "url": "editor.htm"
      }
    ],
    "security": {
      "csp": null,
      "capabilities": ["migrated"]
    }
  }
}
